<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="controller.MainMenuController"
      alignment="CENTER"
      spacing="20"
      style="-fx-background-color: #f0f0f0;">

    <Button text="Cadastrar Cliente" onAction="#openCadastroCliente" style="-fx-font-size: 18px; -fx-min-width: 200px;"/>
    <Button text="Consultar Clientes" onAction="#openConsultaClientes" style="-fx-font-size: 18px; -fx-min-width: 200px;"/>
</VBox>
